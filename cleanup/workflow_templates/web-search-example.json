{
  "id": "web-search-example",
  "name": "Web Search Example",
  "description": "Example workflow that performs web search and processes results",
  "version": "1.0.0",
  "parameters": {
    "query": "default search query",
    "max_results": 5
  },
  "tasks": [
    {
      "id": "search",
      "protocol": "web-search/v1",
      "method": "search",
      "params": {
        "query": "${query}",
        "max_results": "${max_results}"
      },
      "priority": "high",
      "timeout": 30
    },
    {
      "id": "summarize",
      "protocol": "text-processing/v1",
      "method": "summarize",
      "params": {
        "text": "${search.result.combined_text}",
        "max_length": 200
      },
      "dependencies": ["search"],
      "priority": "normal",
      "timeout": 20
    },
    {
      "id": "extract-keywords",
      "protocol": "text-processing/v1", 
      "method": "extract_keywords",
      "params": {
        "text": "${search.result.combined_text}",
        "max_keywords": 10
      },
      "dependencies": ["search"],
      "priority": "low",
      "timeout": 15
    }
  ],
  "metadata": {
    "category": "information-retrieval",
    "tags": ["web-search", "text-processing", "example"]
  }
}